<h1>Show File</h1>

<ul>

  <li>
    <strong>Filename:</strong>
    <%= @file.filename %>
  </li>

  <li>
    <strong>Data:</strong>
    <%= @file.data %>
  </li>

  <li>
    <strong>Size:</strong>
    <%= @file.size %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.course_file_path(@conn, :edit, @course_id, @file) %></span>
<span><%= link "Back", to: Routes.course_file_path(@conn, :index, @course_id) %></span>
